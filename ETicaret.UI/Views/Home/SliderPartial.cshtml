@model IEnumerable<Product>
<!doctype html>
<html lang="en">
  <head>
  	<title>Carousel 09</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800,900" rel="stylesheet">
		
		<link rel="stylesheet" href="~/carousel-09/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/carousel-09/css/owl.theme.default.min.css">
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/4.5.6/css/ionicons.min.css">
		<link rel="stylesheet" href="~/carousel-09/css/style.css">
  </head>
  <body>
		
		<section class="ftco-section">
			<div class="container">
				<div class="row">
					<div class="col-md-12 text-center">
						<h2 class="heading-section mb-5 pb-md-4">Product</h2>
					</div>
					<div class="col-md-12">
						
                        <div class="hero featured-carousel owl-carousel">
                            @foreach (var item in Model)
                        {
                            <div class="item">
								<div class="work">
									<div class="img d-flex align-items-center justify-content-center" style="background-image: url(images/slider-1.jpg);">
										<img src="@await Component.InvokeAsync("Image", new { FileCode = item.ProductPhoto.FileCode })" alt="">
									</div>
								</div>
							</div>
                        }
							
							
						</div>
					</div>
				</div>
			</div>
		</section>

    <script src="~/carousel-09/js/jquery.min.js"></script>
    <script src="~/carousel-09/js/popper.js"></script>
    <script src="~/carousel-09/js/bootstrap.min.js"></script>
    <script src="~/carousel-09/js/owl.carousel.min.js"></script>
    <script src="~/carousel-09/js/main.js"></script>
  </body>
</html>